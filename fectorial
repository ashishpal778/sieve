#include <iostream>
using namespace std;

int main() {
	int n;
		cin>>n;
	bool b[n+1];
	for(int i=2;i<=n;i++)
	b[i]=true;

	for(int i=2;i*i<=n;i++){
	    if(b[i]){
	         for(int j=i*i;j<=n;j+=i){
	              b[j]=false ;
	         }
	    }
	}
	for(int i=2;i<=n;i++)
	{
	     if(b[i]==true)
	     cout<<i<<endl;
	}
	return 0;
}
